{
    "name": "@q6tech/landing",
    "private": true,
    "license": "MIT",
    "repository": "git@github.com:q6tech/q6tech.com.git",
    "author": "Mark Lopez <mark@q6tech.com>",
    "packageManager": "yarn@4.4.0",
    "type": "module",
    "browserslist": ">0.5%, last 2 versions, Firefox ESR, not dead, not op_mini all",
    "importSort": {
        ".ts,.tsx": {
            "style": "import-sort-style-module-compact",
            "options": {}
        }
    },
    "targets": {
        "default": {
            "distDir": "./dist"
        }
    },
    "@parcel/resolver-default": {
        "packageExports": true
    },
    "staticFiles": [
        {
            "staticPath": "src/static/robots.txt",
            "staticOutPath": ""
        }
    ],
    "scripts": {
        "parcel": "parcel",
        "parcel:start": "parcel --port 9999 --config .parcelrc ./src/index.html",
        "parcel:build": "parcel build --config .prod.parcelrc ./src/index.html",
        "sri-dist": "posthtml -u posthtml-sri --posthtml-sri.basePath=dist/ dist/**/*.html",
        "lint": "eslint --max-warnings=0 src",
        "start": "yarn parcel:start",
        "clean": "rimraf ./dist && rimraf ./.cache",
        "build": "yarn clean && yarn parcel:build && yarn sri-dist"
    },
    "devDependencies": {
        "@eslint/js": "^9.9.1",
        "@parcel/optimizer-data-url": "2.12.0",
        "@parcel/packager-raw-url": "2.12.0",
        "@parcel/reporter-bundle-analyzer": "2.12.0",
        "@parcel/transformer-inline-string": "2.12.0",
        "@parcel/transformer-webmanifest": "2.12.0",
        "@tailwindcss/typography": "^0.5.15",
        "@types/eslint__js": "^8.42.3",
        "@types/react": "^18.3.4",
        "@types/react-dom": "^18.3.0",
        "clsx": "^2.1.1",
        "concurrently": "^8.2.2",
        "cross-env": "^7.0.3",
        "eslint": "^9.9.1",
        "eslint-plugin-only-warn": "^1.1.0",
        "eslint-plugin-react": "^7.35.0",
        "nodemon": "^3.1.4",
        "parcel": "2.12.0",
        "parcel-reporter-static-files-copy": "^1.5.3",
        "parcel-resolver-ignore": "^2.2.0",
        "postcss": "^8.4.41",
        "posthtml": "^0.16.6",
        "posthtml-cli": "^0.10.0",
        "posthtml-doctype": "^1.1.1",
        "posthtml-external-link": "^0.1.2",
        "posthtml-sri": "^1.2.1",
        "process": "^0.11.10",
        "rimraf": "^6.0.1",
        "tailwindcss": "^3.4.10",
        "tsx": "^4.19.0",
        "typescript": "^5.5.4",
        "typescript-eslint": "8.3.0"
    },
    "dependencies": {
        "@fontsource-variable/inter": "^5.0.20",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    }
}
